{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "server",
      "image": "682793961433.dkr.ecr.us-east-1.amazonaws.com/service-act:latest",
      "essential": true,
    },
    {
      "name": "proxy",
      "image": "682793961433.dkr.ecr.us-east-1.amazonaws.com/nginx:dev",
      "essential": true,
      "memory": 200,
      "cpu": 1,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "links": [
        "server:server"
      ]
    }
  ]
}